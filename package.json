{
	"name": "hots-portrait-council-discord-bot",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "swc src -d dist --strip-leading-paths",
		"watch": "swc src -d dist --strip-leading-paths -w",
		"check": "biome check",
		"check:knip": "knip",
		"check:types": "tsc --noEmit",
		"dev": "pnpm run build && pnpm run start",
		"generate": "sapphire generate",
		"prepare": "husky",
		"sapphire": "sapphire",
		"start": "node --enable-source-maps --env-file-if-exists=.env dist/index.js",
		"watch:start": "tsc-watch --onSuccess \"pnpm run start\""
	},
	"dependencies": {
		"@sapphire/decorators": "^6.1.1",
		"@sapphire/framework": "^5.3.1",
		"@sapphire/pieces": "4.3.1",
		"@sapphire/plugin-logger": "^4.0.2",
		"consola": "3.3.0",
		"discord.js": "^14.16.3",
		"mlly": "1.7.3",
		"std-env": "3.8.0",
		"valibot": "1.0.0-beta.9",
		"valibot-env": "0.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@sapphire/cli": "^1.9.3",
		"@sapphire/ts-config": "^5.0.1",
		"@swc/cli": "0.5.2",
		"@swc/core": "1.10.1",
		"@total-typescript/tsconfig": "1.0.4",
		"@tsconfig/strictest": "2.0.5",
		"@types/node": "^22.10.2",
		"chokidar": "4.0.3",
		"husky": "9.1.7",
		"knip": "5.41.1",
		"lint-staged": "15.2.11",
		"tsc-watch": "^6.2.1",
		"typescript": "~5.7.2"
	},
	"packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf"
}
